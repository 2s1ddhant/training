{
  "apiVersion":"v1beta3",
  "kind": "Pod",
  "metadata": {
    "name": "hello-openshift"
  },
  "labels": {
    "name": "hello-openshift"
  },
  "spec": {
    "containers": [{
      "name": "hello-openshift",
      "image": "openshift/hello-openshift",
        "resources": {
          "limits": {
            "cpu": "100m",
            "memory": "16Mi"
        },
        "ports": [{
          "name": "hello-openshift",
          "hostPort": 6061,
          "containerPort": 8080,
          "protocol": "TCP"
        }]
      }
    }]
  }
}
